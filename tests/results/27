[38;5;241m    [39m[38;5;252m}[39m

[38;5;241m    [39m[38;5;70;01mlet[39;00m[38;5;241m [39m[38;5;252mup[39m[38;5;241m [39m[38;5;252m=[39m[38;5;241m [39m[38;5;252mDirection[39m[38;5;252m::[39m[38;5;252mUp[39m[38;5;252m;[39m

[38;5;241m    [39m[38;5;248;03m// Enum with fields[39;00m
[38;5;241m    [39m[38;5;70;01menum[39;00m[38;5;252m [39m[38;5;75;04mOptionalI32[39;00m[38;5;241m [39m[38;5;252m{[39m

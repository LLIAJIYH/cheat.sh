[38;5;241m    [39m[38;5;248;03m// Struct[39;00m
[38;5;241m    [39m[38;5;70;01mstruct[39;00m[38;5;252m [39m[38;5;75;04mPoint[39;00m[38;5;241m [39m[38;5;252m{[39m
[38;5;241m        [39m[38;5;252mx[39m[38;5;252m:[39m[38;5;252m [39m[38;5;70;01mi32[39;00m[38;5;252m,[39m
[38;5;241m        [39m[38;5;252my[39m[38;5;252m:[39m[38;5;252m [39m[38;5;70;01mi32[39;00m[38;5;252m,[39m
[38;5;241m    [39m[38;5;252m}[39m

[38;5;241m    [39m[38;5;70;01mlet[39;00m[38;5;241m [39m[38;5;252morigin[39m[38;5;252m:[39m[38;5;252m [39m[38;5;75;04mPoint[39;00m[38;5;241m [39m[38;5;252m=[39m[38;5;241m [39m[38;5;252mPoint[39m[38;5;241m [39m[38;5;252m{[39m[38;5;241m [39m[38;5;252mx[39m[38;5;252m:[39m[38;5;252m [39m[38;5;75m0[39m[38;5;252m,[39m[38;5;241m [39m[38;5;252my[39m[38;5;252m:[39m[38;5;252m [39m[38;5;75m0[39m[38;5;241m [39m[38;5;252m}[39m[38;5;252m;[39m

[38;5;241m    [39m[38;5;248;03m// A struct with unnamed fields, called a â€˜tuple structâ€™[39;00m
[38;5;241m    [39m[38;5;70;01mstruct[39;00m[38;5;252m [39m[38;5;75;04mPoint2[39;00m[38;5;252m([39m[38;5;70;01mi32[39;00m[38;5;252m,[39m[38;5;241m [39m[38;5;70;01mi32[39;00m[38;5;252m)[39m[38;5;252m;[39m

[38;5;241m    [39m[38;5;70;01mlet[39;00m[38;5;241m [39m[38;5;252morigin2[39m[38;5;241m [39m[38;5;252m=[39m[38;5;241m [39m[38;5;252mPoint2[39m[38;5;252m([39m[38;5;75m0[39m[38;5;252m,[39m[38;5;241m [39m[38;5;75m0[39m[38;5;252m)[39m[38;5;252m;[39m

[38;5;241m    [39m[38;5;248;03m// Basic C-like enum[39;00m
[38;5;241m    [39m[38;5;70;01menum[39;00m[38;5;252m [39m[38;5;75;04mDirection[39;00m[38;5;241m [39m[38;5;252m{[39m
[38;5;241m        [39m[38;5;252mLeft[39m[38;5;252m,[39m
